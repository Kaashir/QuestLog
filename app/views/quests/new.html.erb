<div class="container">
  <h4>Add custom quest</h4>
  <%= simple_form_for(@quest) do |f| %>
    <%= f.input :title, label: "Quest Name" %>
    <%= f.input :description, label: "Quest Description" %>
    <%# <%= f.input :category, label: "Quest Category" %>
    <%= f.input :frequency, label: "Frequency (per week)" %>
    <%= link_to "Go back", new_user_quest_path %>
    <%= f.submit "Create quest" %>
  <% end %>
</div>
